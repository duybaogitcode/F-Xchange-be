type Query{
    getUserById(id:ID): User
}

type User{
    id: ID
    information: Information
    googleId: String
    role:Role
    point: Int
    invitationCode:String
    status:String
    createAt:String
    updateAt:String
    attendanceDates:[String]
}

type Information{
    email: String
    fullname: String
    phone:[String]
    address:[String]
    avatarUrl: String
}

type Role{
    name:String
}